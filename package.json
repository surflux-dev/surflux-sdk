{
  "name": "@surflux/sdk",
  "version": "0.1.0",
  "description": "Type-safe Sui event streaming with Surflux integration",
  "publishConfig": {
    "access": "restricted"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "bin": {
    "surflux-sdk": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsc",
    "prepare": "npm run build",
    "prepublishOnly": "npm run build",
    "dev": "tsc --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "test": "jest",
    "test:unit": "jest --testPathPattern=test/unit",
    "test:e2e": "jest --testPathPattern=test/e2e",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "sui",
    "blockchain",
    "events",
    "surflux",
    "typescript",
    "types"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@mysten/sui": "^1.45.0",
    "axios": "^1.13.2",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "eventsource-client": "^1.0.0",
    "fs-extra": "^11.2.0"
  },
  "peerDependencies": {
    "typescript": ">=4.0.0"
  },
  "browser": {
    "fs-extra": false,
    "path": false
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.4",
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@typescript-eslint/parser": "^8.52.0",
    "eslint": "^9.39.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist",
    "README.md"
  ]
}